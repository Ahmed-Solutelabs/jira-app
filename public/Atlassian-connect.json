{
    "name": "Hello KYROS TEAM",
    "description": "Atlassian Connect app",
    "key": "com.example.myapp",
    "baseUrl": "https://jira-app-inky.vercel.app/",
    "vendor": {
        "name": "Example, Inc.",
        "url": "https://jira-app-inky.vercel.app/"
    },
    "authentication": {
        "type": "none"
    },
    "apiVersion": 1,
    "modules": {
        "generalPages": [
            {
                "url": "/index.html",
                "key": "hello-world",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "KYROS TESTING"
                }
            }
        ],
       "jiraIssueContexts": [
      {
        "icon": {
          "url": "https://jira.use2.clockify.me/images/clockify-icon.png"
        },
        "content": {
          "type": "label",
          "label": {
            "value": "Start / Stop"
          }
        },
        "target": {
          "type": "web_panel",
          "url": "stopwatch?exclusive=true"
        },
        "name": {
          "value": "Clockify"
        },
        "key": "clk-stopwatch",
        "layout": {
          "top": "0px",
          "width": "100%",
          "height": "100%"
        },
        "jiraNativeAppsEnabled": true
      }
    ],
    "webPanels": [
      {
        "url": "stopwatch",
        "location": "atl.jira.view.issue.right.context",
        "layout": {
          "height": "100%",
          "width": "100%"
        },
        "name": {
          "value": "Clockify"
        },
        "key": "clk-stopwatch-legacy"
      },
      {
        "url": "stopwatch",
        "location": "atl.gh.issue.details.tab",
        "layout": {
          "height": "100%",
          "width": "100%"
        },
        "name": {
          "value": "Clockify"
        },
        "key": "clk-stopwatch-legacy-gh"
      }
    ]
    }
}
